<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h2>フォローリスト</h2>
      <% current_user.following_user.where.not(id: current_user.id).each do |user| %>
      <table class="table">
        <tbody>
          <tr>
            <th>ユーザー名</th>
            <td><%= link_to user.name, user_path(user) %></td>
          </tr>
        </tbody>
      </table>
    <% end %>
    </div>
  </div>
</div>